{
    class Program
    {
        static void Main(string[] args)
        {
            int N, M;
            int result = 0;
            N = Int32.Parse(Console.ReadLine());
            M = Int32.Parse(Console.ReadLine());
            while (N >= M)
            {
                N -= M;
                result++;
            }
            Console.WriteLine("Частное: {result},остаток: {N}");
