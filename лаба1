/*
using System;

class Program
{
	static void Main()
	{
		double x = 0.5; 
		double epsilon = 0.0001; 
		double sum = 0.0;
		double term = 1.0; 
		int n = 1;

		while (Math.Abs(term) >= epsilon)
		{
			sum += term;
			n++;
			term = Math.Cos(n * x) / (n * n);
		}

		Console.WriteLine(sum);
	}
}
*/
#endregion

